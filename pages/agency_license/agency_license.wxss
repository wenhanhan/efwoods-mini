/* pages/agency_license/agency_license.wxss */
page{
  background: #eeeeee;
}
.info_box{
  width: 700rpx;
  height: 900rpx;
  background: white;
  display: flex;
  align-items: center;
  flex-direction: column;
  border-radius: 16rpx;
  margin-top: 20rpx;
}
.info_box_top{
  width: 90%;
  height: 140rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-direction: column;
  border-bottom: 1px solid #D7D6D6;
}
.info_box_bottom{
  width: 90%;
  height: 740rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-direction: column;
}
.contact{
  width: 100%;
  font-size: 30rpx;
  color: #686565;
  font-weight: 600;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-top: 30rpx;
  margin-bottom: 12rpx;
}
.contact_name{
   width: 100%;
  font-size: 28rpx;
  color: #7D7B7B;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-bottom: 16rpx;
}
.license{
  width: 100%;
  height: auto;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 30rpx;
  color: #686565;
  font-weight: 600;
  margin-top: 30rpx;
}
.license_box{
  width: 420rpx;
  height: 600rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 20rpx;
}
.license_box image{
  width: 100%;
  height: 100%;
}