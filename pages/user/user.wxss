/* pages/user/user.wxss */
@import '/icon/level1.wxss';
@import '/icon/level2.wxss';
@import '/icon/level3.wxss';
@import '/icon/level4.wxss';
@import '/icon/level5.wxss';
@import '/icon/level6.wxss';
@import '/icon/huiyuan.wxss';
@import '/icon/rank.wxss';
@import '/icon/shop.wxss';
page{
  background: #eeeeee;
}
.top{
  background-image:url('data:image/jpeg;base64,');background-repeat:no-repeat;background-size:100% 100%;width:100%;height:300rpx;font-size:72rpx;color:#fff;border-bottom-right-radius: 90rpx;border-bottom-left-radius: 90rpx;display: flex;
  justify-content: center;
  align-items: center;position: relative;
}
.name{
  color: white;
  width: auto;
  font-size: 42rpx;
  font-weight: 600;
  display: flex;
  justify-content: center;
  align-items: center;
}
.headimg{
  width: 160rpx;
  height: 160rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  /* border: 2rpx solid white; */
  position: absolute;
  border-radius: 80rpx;
  bottom: -60rpx;
  background: white;
}
.headimg image{
  width: 100%;
  height: 100%;
  border-radius: 80rpx;
  box-sizing: border-box;
  border: 6rpx solid white;
}
.user_data{
  width: 100%;
  height: auto;
  color: #464545;
  font-size: 24rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin-top: 100rpx;
  margin-bottom: 20rpx;
  position: relative;
  
}
.fensi{
  position: absolute;
  left: 50rpx;
}
.qiandao{
  position: absolute;
}
.show{
  position: absolute;
  right: 50rpx;
}

.my_level{
  width: 95%;
  height: 180rpx;
  background: white;
  border-radius: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  margin-top: 30rpx;
}
.cur_level{
  color: #444343;
  font-size: 28rpx;
  width: auto;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cur_exp{
  color: #444343;
  font-size: 28rpx;
  width: auto;
  display: flex;
  justify-content: center;
  align-items: center;
}
.my_level_top{
  width: 90%;
  height: auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.my_level_bottom{
  width: 95%;
  height: auto;
  display: flex;
  justify-content: center;
  align-items: center;
}
.exp_line{
  width: 100%;
  height: auto;
}
.i-progress-inner{
  background-color: #ffb0b9 !important;
}
.i-progress-bg{
  background-color: #ff263f !important;
}

.qujian{
  width: 95%;
  height: 200rpx;
  background: white;
  border-radius: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  margin-top: 20rpx;
  padding-bottom: 20rpx;
}
.qujian_title{
  font-size: 28rpx;
  width: 90%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  color: #444343;
}
.line_box{
  width: 90%;
  height: auto;
  display: flex;
  justify-content: center;
}
.line{
  width: 100%;
  height: 18rpx;
  border-radius: 9rpx;
  background-color: #ff0061; /* 不支持线性的时候显示 */
  background-image: linear-gradient(to right, #ffae54,#ff0061);
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.level_txt_box{
  width: 90%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 6rpx;
  margin-top: 10rpx;
}
.level_txt{
  font-size: 26rpx;
  color: #FF263F;
}
.node{
  width: 30rpx;
  height: 30rpx;
  border-radius: 15rpx;
  background: #FDED4E;
  margin: 0 -2rpx;
}
.exp{
  width: 95%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 10rpx;

}
.exp_txt{
  color: #FF263F;
  font-size: 28rpx;
}
.quanyi{
  width: 95%;
  height: auto;
  background: white;
  border-radius: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  margin-top: 20rpx;
  margin-bottom: 30rpx;
}
.item{
  width: 100%;
  height: 180rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-bottom: 1px solid #eeeeee;
}
.line_left{
  width: 24%;
  height: 100%;
  display: flex;
  /* background: #FFE19A; */
  align-items: center;
  justify-content: center;
}
.line_left_square{
  width: 130rpx;
  height: 130rpx;
  border-radius: 16rpx;
  background: #FFE19A;
  display: flex;
  justify-content: center;
  align-items: center;
}
.line_right{
  width: 76%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.quanyi_title{
  width: 100%;
  height: auto;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 36rpx;
  color: #090909;
  font-weight: 600;
}
.quanyi_des{
  width: 100%;
  height: auto;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 28rpx;
  color: #4B4A4A;
  /* font-weight: 600; */
}
.iconfont_level1{
  font-size: 32rpx;
}
