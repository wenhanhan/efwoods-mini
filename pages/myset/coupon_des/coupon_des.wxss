page{
  background-color: #eeeeee;
}
.container{
  font-family: Microsoft YaHei;
}
.tick_title{
  width: 100%;
  height: 80rpx;
  line-height: 75rpx;
  float: left;
  position: relative;
  background: white;
  font-size: 28rpx;
  color:#333333;
  padding-left: 28rpx;
  border-bottom: 1px solid #eeeeee;
}
.tick_des_box{
  width: 100%;
  height: auto;
  float: left;
  background: white;
  font-size: 28rpx;
}
.tick_line{
  width: 100%;
  height: auto;
  display: flex;
  justify-content: center;
  align-items: flex-start;
}
.tick_line_left{
  width: 20%;
  height: auto;
  display: flex;
  justify-content: center;
  align-items: flex-start;
  font-size: 26rpx;
  color:#333333;
  padding: 10rpx 0;
}
.tick_line_right{
  width: 80%;
  height: auto;
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  font-size: 24rpx;
  color: #666666;
  padding: 10rpx 0;
}
.tick_code{
  width: 170rpx;
  height: 170rpx;
}
.canvas{
  position: fixed;
  top: 0;
  left: 800px;
}
