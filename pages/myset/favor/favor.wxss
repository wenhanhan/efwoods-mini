page{
  /* background-color: #eeeeee; */
  background-color:white;
}
.container{
  font-family: Microsoft YaHei;
}
.nav_fix{
  width:100%;
  height:84rpx;
  position: fixed;
  z-index: 999;
}
.favor_box{
  width: 95%;
  height: auto;
  margin: 0 auto;
  margin-top: 84rpx;
  position: relative;
}
.remove_favor{
  width: 60rpx;
  height: 60rpx;
  display: flex;
  top: 10rpx;
  justify-content: center;
  align-items: center;
  position: absolute;
  bottom: 80rpx;
  right: 68rpx;
  z-index: 1;
}

/* 视频收藏 */
/* 教练秀 */
.home_show_video{
  width: 100%;
  height: auto;
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  align-items: center;
}

.child_video_box{
  width: 94%;
  height: auto;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: center;
}

.home_video_box{
  width: 46%;
  height: 230rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  /* flex-direction: column; */
  /* margin-top: 20rpx; */
}

.home_video_box image{
  width: 100%;
  height: 180rpx;
  border-radius: 10rpx;
}
.video_des{
  width: 100%;
  height: 90rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-direction: column;
}
.video_title{
  width: 100%;
  height: auto;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 28rpx;
  color: #1D1C1C;
}
.video_play_times{
  width: 100%;
  height: auto;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 20rpx;
  color: #1D1C1C;
}