/* pages/myset/spread_notes/spread_notes.wxss */
@import '/icon/search.wxss';
@import '/icon/lost.wxss';
.container{
  overflow-x: hidden;
  padding-bottom: 30rpx;
}
.top_bk{
  width: 100%;
  height: 280rpx;
  position: relative;
  background-image:url('https://cdn.icloudapi.cn/spread_top_bg.png'); 
  background-repeat:no-repeat;background-size:100% 100%;
  display: flex;
  justify-content: center;
  align-items:center;
}
.record_detail{
  width: 100%;
  height: auto;
  position: absolute;
  bottom: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  color: white;
}
.detail_type{
  width: 33%;
  height: auto;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  flex-direction: column;
}
.line{
  width: 1px;
  height: 60rpx;
  background: white;
}
.tab{
  width: 100%;
  height: 84rpx;
}
.level_tab{
  width: 100%;
  height: 70rpx;
  border-bottom: 6rpx solid #eeeeee;
}
.search{
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-bottom: 1px solid #eeeeee;
  position: relative;
}
.placeholder{
  font-size: 24rpx;
  color: #949393;
}
.search_input{
  width: 80%;
  height: 60rpx;
  border-radius: 30rpx;
  background: #eeeeee;
  text-align: center;
  font-size: 26rpx;
}
.search_icon{
  width:auto;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  right: 20rpx;
}
.sijiao,.agency{
  width: 100%;
  height: auto;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-direction: column;
}
.sijiao_item{
  width: 100%;
  height: 140rpx;
  display: flex;
  justify-content: flex-start;
  align-items:center;
  padding-left: 10%;
  border-bottom: 1px solid #eeeeee;
}
.sijiao_item image{
  width: 92rpx;
  height: 92rpx;
  border-radius: 46rpx;
}
.sijiao_des,.agency_des{
  width: 70%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  padding-left: 20rpx;
}
.sijiao_name{
  width: 100%;
  height: 50%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 26rpx;
  color: #333232;
}
.time,.agency_time{
  width: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 20rpx;
  color: #686868;
}
.agency_item{
  width: 100%;
  height: 200rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding-left: 10%;
  border-bottom: 1px solid #eeeeee;
}
.agency_item image{
  width: 270rpx;
  height: 160rpx;
  border-radius: 16rpx;
}
.agency_name{
  width: 100%;
  height: 40%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 36rpx;
  color: #0F0F0F;
  margin-bottom: 40rpx;
}
