/* pages/myset/personal/personal.wxss */
@import '/icon/child.wxss';
@import '/icon/course.wxss';
.container{
  overflow-x: hidden;
  margin-bottom: 30rpx;
}
page{
  background-color: #eeeeee;
}
.cell_line{
  width: 100%;
  float: left;
  position: relative;
  margin-top: 20rpx;
}

.child_info{
  width: 100%;
  height: 98rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-top: 20rpx;
  background: white;
  font-size: 28rpx;
}
.weui-uploader{
  width: 100%;
  float: left;
  position: relative;
  background: white;
  border-top: 1px solid #eeeeee;
  font-size: 28rpx;
}
.weui-uploader__bd{
  margin-left:30rpx;
  margin-top:30rpx;
}
.delete{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: 0;
  right: 0;
}
.weui-uploader__file{
  position: relative;
}
.iconfont_guanbi{
  color: #EC5A5A;
}

.tea_info{
  width: 100%;
  height: 98rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-top: 20rpx;
  background: white;
  font-size: 28rpx;
}

.save_btn{
  width: 90%;
  height: 80rpx;
  border-radius: 50rpx;
  background: black;
  color: #f2d11b;
  text-align: center;
  line-height: 80rpx;
  font-size: 28rpx;
  position: relative;
  margin:0 auto;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.btn_box{
  width: 100%;
  height: auto;
  float: left;
  position: relative;
  /* background: white; */
  bottom: 0;
  border-top: 1px solid #eeeeee;
  margin-top: 30rpx;
}

/* 上传框样式 */

.weui-uploader{
  width: 100%;
  float: left;
  position: relative;
  background: white;
  border-top: 1px solid #eeeeee;
  font-size: 28rpx;
}
.weui-uploader__hd{
  padding-bottom: 0;
  padding-left: 15px;
  padding-top: 12px;
}
.weui-uploader__bd{
  margin-left:30rpx;
  margin-top:30rpx;
}
.delete{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: 0;
  right: 0;
}
.weui-uploader__file{
  position: relative;
}
.phone_btn{
background:white;
width:100%;
height:96rpx;
position:absolute;
bottom:0;
margin:0 auto;
opacity:0;
}