/* 筛选区域样式 */

.shaixuan{
  display: none;
  width: 100%;
  position: relative;
  z-index: 4;
  max-height: 750rpx;
  overflow-y: scroll;
  /* line-height: 145rpx; */
  background: #fff;
}

@keyframes slidown {
  from {
    transform: translateY(-100%);
  }

  to {
    transform: translateY(0%);
  }
}

.slidown {
  display: block;
  animation: slidown 0.3s ease-in both;
  position: fixed;
  top: 175rpx;
}

@keyframes slidup {
  from {
    transform: translateY(0%);
  }

  to {
    transform: translateY(-100%);
  }
}

.slidup {
  display: block;
  animation: slidup 0.3s ease-in both;
}

.disappear {
  display: none;
}


