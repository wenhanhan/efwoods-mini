/* 教师列表样式 */
.tea_box{
  width: 95%;
  height: auto;
  position: absolute;
  top:190rpx;
}
.tea_list{
  width: 100%;
  height: 200rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
  margin-bottom: 30rpx;
  border-radius: 18rpx;
  background: #FAF3D7;
}

.list_left{
  width: 20%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.list_right{
  width: 80%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}
.set_tea_img{
  width: 96rpx;
  height: 96rpx;
  border-radius: 48rpx;
}
.set_tea_img image{
  width: 100%;
  height: 100%;
  border-radius: 48rpx;
}
.right_line{
  width: 100%;
  height: 25%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.set_tea_name,.set_tea_address,.set_tea_phone,.set_tea_level,.courNum,.set_tea_type{
  width: 48%;
  height: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 24rpx;
  color: #252424;
  overflow: hidden;
text-overflow:ellipsis;
white-space: nowrap;
}
.applyTime{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 24rpx;
  color: #252424;
}
.tea_set{
  position: absolute;
  display: flex;
  justify-content: center;
  align-items: center;
  bottom: 0;
  right: 35rpx;
}
.set_txt{
  font-size: 24rpx;
  color: #252424;
}

