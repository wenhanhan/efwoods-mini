@import '/icon/check.wxss';
page{
  background-color: #eeeeee;
}
.container{
  font-family: Microsoft YaHei;
}


.coupon_title{
  width: 100%;
  height: 70rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  background: white;
  padding-bottom: 10rpx;
}
.used_tick,.use_tick{
  width: 35%;
  height: 100%;
  display: flex;
  align-items: flex-end;
  justify-content: center;
  /* background: red; */
}
.used_tick_box{
  width: 46rpx;
  height: 38rpx;
}
.used_tick_box image{
  width: 100%;
  height: 100%;
}
.use_tick_box{
  width: 55rpx;
  height: 37rpx;
}
.use_tick_box image{
  width: 100%;
  height: 100%;
}
.tick_list_box{
  width: 100%;
  height: auto;
  float: left;
  background: white;
  position: relative;
}

.tick_list{
  width: 93%;
  height: 120rpx;
  border-radius: 10rpx;
  background: #f9f5eb;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0 auto;
  margin-top: 30rpx;
  margin-bottom: 10rpx;
  position: relative;
}

.tick_down{
  width: 93%;
  height: auto;
  border-radius: 10rpx;
  background: #f9f5eb;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  margin: 0 auto;
  padding: 10rpx 0;
  margin-top: 10rpx;
  font-size: 22rpx;
  color: #333333; 
  margin-bottom: 30rpx;
  position: relative;
}

.tick_list_left{
  width:18%;
  height: 100%; 
  /* background: red; */
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.tick_list_right{
  width: 82%;
  height: 100%;
  /* background: green; */
}
.tick_money{
  width: 100%;
  height: 50%;
  display: flex;
  justify-content: center;
  align-items: flex-end;
}
.tick_type{
  width: 100%;
  height: 50%;
  display: flex;
  justify-content: center;
  align-items: flex-start;
  font-size: 20rpx;
  color: #333333;
  font-weight:500
}
.divide{
  width: 1px;
  height: 38rpx;
  background: #999999;
}
.tick_title{
  width: 100%;
  height: 50%;
  justify-content: flex-start;
  align-items: flex-end;
  font-size: 28rpx;
  display: flex;
  color: #333333;
  font-weight: 500;
  margin-left: 26rpx;
}
.tick_des{
  width: 100%;
  height: 50%;
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  flex-direction: column;
  font-size: 22rpx;
  color: #666666; 
  margin-left: 26rpx;
}
.tick_state{
  width: 100%;
  height: 50%;
  display: flex;
  font-size: 18rpx;
  justify-content: flex-start;
  align-items: center;
  position: relative;
}
.deadline{
  width: 100%;
  height: 50%;
  font-size: 18rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  position: relative;
}



.state_btn{
  width: 140rpx;
  height: 45rpx;
  border-radius: 25rpx;
  position: absolute;
  right: 36rpx;
  background: #ff0000;
  color: white;
  text-align: center;
  line-height: 45rpx;
  font-size: 24rpx;
  padding: 0;
}

.check_box{
  width: 100%;
  height: auto;
  float: left;
  background: white;
  position: relative;
  /* margin-top: 20rpx; */
}
.check_child_box{
   width: 93%;
   height: auto;
  border-radius: 10rpx;
  background: #f9f5eb;
  display: flex;
  align-items: flex-start;
  justify-content: center;
  flex-direction: column;
  margin: 0 auto;
  position: relative;
  margin-bottom: 20rpx;
}

.record_title{
  width: 100%;
  height: 70rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  border-bottom: 1px solid white;
  /* font-size: 24rpx; */
  color: #333333;
  padding-left: 30rpx;
}
.record_item{
  width: 100%;
  height: 70rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
  color: #333333;
  border-bottom: 1px solid white;
}

.record_head,.record_nick{
  width: 20%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.get_time,.check_time{
  width: 30%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.record_des{
  width: 100%;
  height: 70rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 18rpx;
  color: #333333;
  border-bottom: 1px solid white;
}
.record_des image{
  width: 52rpx;
  height: 52rpx;
  border-radius: 26rpx;
}
